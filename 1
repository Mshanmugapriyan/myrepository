/*this is objective c code to check a number is positive(or)negative (or)zero*/
#include<Foundation.h>
int main()
{
  int a=5;/*to execute change the number here*/
  if(a!=0)
  {
     if(a>0)
     {
      NSLog(@"POSITIVE NUMBER");
     }
     else
     {
     NSLog(@"NEGATIVE NUMBER");
     }
  }
  else
  {
   NSLog(@"THE NUMBER IS ZERO");
  }
  return 0;
}
